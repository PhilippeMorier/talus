{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "editor": {
      "root": "projects/editor/",
      "sourceRoot": "projects/editor/src",
      "projectType": "application",
      "prefix": "tls",
      "schematics": {
        "@schematics/angular:component": {
          "styleext": "scss"
        }
      },
      "architect": {
        "benchmark": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/editor/src/benchmark.ts",
            "polyfills": "projects/editor/src/polyfills.ts",
            "tsConfig": "projects/editor/tsconfig.benchmark.json",
            "karmaConfig": "projects/editor/karma.benchmark.conf.js",
            "styles": [],
            "scripts": [],
            "assets": []
          }
        },
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/editor",
            "index": "projects/editor/src/index.html",
            "main": "projects/editor/src/main.ts",
            "polyfills": "projects/editor/src/polyfills.ts",
            "tsConfig": "projects/editor/tsconfig.app.json",
            "assets": ["projects/editor/src/favicon.ico", "projects/editor/src/assets"],
            "styles": ["projects/editor/src/styles.scss"],
            "scripts": [],
            "es5BrowserSupport": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/editor/src/environments/environment.ts",
                  "with": "projects/editor/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "editor:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "editor:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "editor:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/editor/src/test.ts",
            "polyfills": "projects/editor/src/polyfills.ts",
            "tsConfig": "projects/editor/tsconfig.spec.json",
            "karmaConfig": "projects/editor/karma.conf.js",
            "styles": ["projects/editor/src/styles.scss"],
            "scripts": [],
            "assets": ["projects/editor/src/favicon.ico", "projects/editor/src/assets"]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["projects/editor/tsconfig.app.json", "projects/editor/tsconfig.spec.json"],
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    }
  },
  "defaultProject": "editor"
}
